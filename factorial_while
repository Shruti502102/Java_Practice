import java.util.*;
public class factorial_while {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int fact=1;
        int i=1;
        int a=sc.nextInt();
        while(a>=i){
            fact=fact*i;
            i++;
        }
        System.out.println("Factorial of "+a+" is "+fact);    
    } 
}
