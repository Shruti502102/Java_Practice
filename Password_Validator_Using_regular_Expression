import java.util.*;
public class passwordvalidator {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String password=sc.nextLine();
        boolean valid=password.matches("^(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[@#$%^&+-]).{8,}$");
        System.out.println(valid);
    }   
}
