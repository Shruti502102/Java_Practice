import java.util.Scanner;
public class sumofdigits {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int s=0;
        while(a!=0){
            s=s+a%10;//taken last to calculate
            a=a/10;//remove last
        }
        System.out.println(s);       
    }
}
